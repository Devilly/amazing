    ds_list_add(graphData, rowData);
    ds_list_mark_as_list(graphData, ds_list_size(graphData) - 1);
    
    show_debug_message("row " + string(yIndex + 1) + " of " + string(sprite_height) + " rows");
}

var map = ds_map_create();
ds_map_add_list(map, "graphData", graphData);

var fileLocation = working_directory + "graphData.json";
var file = file_text_open_write(fileLocation);
file_text_write_string(file, json_encode(map));
file_text_close(file);

show_debug_message("file written to " + fileLocation);

game_end();
