height = obj_combat_background.sprite_height;
view_wview[2] = width;
view_hview[2] = height;

view_wport[2] = width;
view_hport[2] = height;

// init playground
var spriteWidth = sprite_get_width(spr_maze);
var spriteHeight = sprite_get_height(spr_maze);

global.columns = 4;
global.rows = 5;

global.upperLeftX = 0;
global.upperLeftY = 5000;
global.lowerRightX = global.upperLeftX + global.columns * spriteWidth;
global.lowerRightY = global.upperLeftY + global.rows * spriteHeight;

view_xview[0] = global.upperLeftX;
view_yview[0] = global.upperLeftY;

var zoomOfView = 1.5;
view_wview[0] = windowWidth * zoomOfView;
view_hview[0] = windowHeight * zoomOfView;

view_wport[0] = windowWidth;
view_hport[0] = windowHeight;

var spriteIndex = 0;
var yIndex;
for(yIndex = 0; yIndex &lt; global.rows; yIndex++) {
    var xIndex;
    for(xIndex = 0; xIndex &lt; global.columns; xIndex++) {
        var instance = instance_create(global.upperLeftX + xIndex * spriteWidth,
            global.upperLeftY + yIndex * spriteHeight, obj_maze);
        instance.image_speed = 0;
        instance.image_index = spriteIndex;
        
        instance = instance_create(global.upperLeftX + xIndex * spriteWidth,
            global.upperLeftY + yIndex * spriteHeight, obj_maze_hitbox);
        instance.image_speed = 0;
        instance.image_index = spriteIndex;
        
        instance = instance_create(global.upperLeftX + xIndex * spriteWidth,
            global.upperLeftY + yIndex * spriteHeight, obj_sand);
        instance.image_speed = 0;
        instance.image_index = spriteIndex;
        
        spriteIndex++;
    }
}

